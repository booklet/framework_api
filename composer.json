{
  "name": "booklet/framework",
  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "booklet/framework",
        "version": "0.1.0",
        "type": "package",
        "source": {
          "url": "git@github.com/booklet/framework.git",
          "type": "git",
          "reference": "master"
        }
      }
    },
    {
      "type": "git",
      "url": "https://github.com/booklet/utils.git"
    },
    {
      "type": "git",
      "url": "https://github.com/booklet/graphics-tools.git"
    }
  ],
  "require": {
    "booklet/utils": "dev-master",
    "booklet/graphics-tools": "dev-master",
    "matthiasmullie/minify": "1.3.46"
  },
  "require-dev": {
    "phpunit/phpunit": "^6"
  },
  "autoload": {
    "classmap": [
        "src", "lib"
    ],
    "files": [
        "src/global_functions.php"
    ]
